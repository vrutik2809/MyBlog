<!DOCTYPE html>
<html lang="en">
<!-- header -->
<%- include('./partials/header') %>
<body>
    <!-- nav bar -->
    <%- include('./partials/nav') %>  
    <div class="content">
        <% if (data.length > 0) { %>
            <% data.forEach(data => { %>
                <a href="/user/<%= data.username %>/blogs/<%= data._id %> ">
                    <h3> <%= data.title %> </h3>
                    <p> <%= data.overview %> </p>
                </a>
            <% }) %>
        <% } else {%>
            <p>You don't have any blog</p>
        <% } %> 
    </div>
    <!-- footer -->
    <%- include('./partials/footer') %>
</body>
</html>